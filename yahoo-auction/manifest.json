{
    "name": "HelpfeelYahoo",
    "short_name": "HelpfeelYahoo",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Helpfeel Yahoo-auction",

    "permissions": [
    ],

    "icons": {
	"48": "icons/helpfeel-48.png",
	"96": "icons/helpfeel-96.png",
	"128": "icons/helpfeel-128.png"
    },

    "content_scripts": [
	{
	    "matches": [ "https://www.yahoo-help.jp/*" ],
	    "js": ["jquery-2.1.4.min.js", "asearch.js", "helpfeel.js", "data.js", "shuffle.js", "search.js"],
	    "run_at": "document_idle"
	}
    ]
}

