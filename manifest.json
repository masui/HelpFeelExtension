{
    "name": "Helpfeel",
    "short_name": "Helpfeel",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Helpfeel",

    "permissions": [
    ],

    "icons": {
	"48": "icons/realurl-48.png",
	"96": "icons/realurl-96.png"
    },

    "content_scripts": [
	{
	    "matches": [ "https://www.yahoo-help.jp/*" ],
	    "js": ["jquery-2.1.4.min.js", "asearch.js", "helpfeel.js", "yahoo-auction.js", "shuffle.js", "search.js"],
	    "run_at": "document_idle"
	}
    ]
}

